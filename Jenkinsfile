pipeline{
  agent any
  stages{
    stage('stage1'){
      steps{
        echo 'Test for Jenkinsfile'
      }
    }
    stage('stage2'){
      steps{
        echo 'The Jenkins file is working!!'
      }
    }
    stage('stage3'){
      steps{
        echo 'Now let's build a real CI/CD pipeline'
      }
    }
  }
}
